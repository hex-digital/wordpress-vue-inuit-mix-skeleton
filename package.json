{
  "name": "skeleton-theme",
  "repository": "git@github.com:hex-digital/skeleton.git",
  "version": "1.0.2",
  "description": "Skeleton Theme to give a jump start into a bespoke WordPress theme",
  "main": "webpack.mix.js",
  "private": false,
  "scripts": {
    "phpcs": "vendor/bin/phpcs --extensions=php .",
    "jslint": "yarn eslint resources/js --ext vue,js",
    "csslint": "yarn stylelint resources/sass",
    "lint": "echo 'Running phpcs' && yarn phpcs && echo 'phpcs complete' && echo 'Running eslint' && yarn jslint && echo 'eslint complete' && echo 'Running stylelint' && yarn csslint && echo 'stylelint finished'",
    "dev": "npm run development",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --watch --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch-poll": "npm run watch -- --watch-poll",
    "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "npm run production",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
  },
  "author": "Hex Digital Ltd",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0",
    "debounce": "^1.2.0",
    "inuitcss": "^6.0.0",
    "throttleit": "^1.0.0",
    "vue": "^2.5.22",
    "vue-analytics": "^5.16.2",
    "vue-analytics-fb": "^1.1.2",
    "vue-axios": "^2.1.4",
    "vuex": "^3.1.0"
  },
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.28",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "clean-webpack-plugin": "^1.0.0",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jest": "^22.1.3",
    "eslint-plugin-vue": "^5.1.0",
    "imagemin-mozjpeg": "^7.0.0",
    "imagemin-webpack-plugin": "^2.1.0",
    "jest": "^23.6.0",
    "jest-serializer-vue": "^2.0.2",
    "jest-transform-stub": "^1.0.0",
    "laravel-mix": "^2.1.10",
    "purify-css": "^1.2.5",
    "purifycss-webpack": "^0.7.0",
    "sass-resources-loader": "^2.0.0",
    "stylelint": "^9.1.3",
    "stylelint-order": "^2.0.0",
    "stylelint-scss": "^2.5.0",
    "stylelint-webpack-plugin": "^0.10.3",
    "svg-spritemap-webpack-plugin": "^1.1.1",
    "vue-jest": "^3.0.2",
    "vue-server-renderer": "^2.5.22",
    "vue-template-compiler": "^2.5.22",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-shell-plugin": "^0.5.0"
  }
}
